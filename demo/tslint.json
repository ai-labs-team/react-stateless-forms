{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb",
    "tslint-react",
    "tslint-microsoft-contrib",
    "tslint-consistent-codestyle"
  ],
  "linterOptions": {
    "exclude": "/e2e"
  },
  "jsRules": {
    "array-bracket-spacing": [
      true,
      "never"
    ],
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "ban": [
      true,
      {
        "name": "alert",
        "message": "BAD! NO ALERT!!!!! SHAME!!!!"
      }
    ],
    "block-spacing": true,
    "brace-style": false,
    "eofline": true,
    "guard-for-in": false,
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": [
          "**/*_test.js"
        ]
      }
    ],
    "import/prefer-default-export": false,
    "indent": [
      true,
      "spaces",
      2
    ],
    "jsx-a11y/label-has-for": [
      "error",
      {
        "required": {
          "some": [
            "nesting",
            "id"
          ]
        }
      }
    ],
    "jsx-a11y/no-static-element-interactions": [
      false,
      {}
    ],
    "jsx-boolean-value": [
      true,
      "never"
    ],
    "jsx-ban-props": [
      true,
      {
        "forbid": [
          "any"
        ]
      }
    ],
    "jsx-key": true,
    "jsx-self-close": true,
    "linebreak-style": [
      false,
      "unix"
    ],
    "newline-before-return": false,
    "no-duplicate-imports": true,
    "no-duplicate-variable": true,
    "no-empty": false,
    "no-extra-semi": [
      true,
      "always"
    ],
    "no-invalid-this": true,
    "no-multi-spaces": true,
    "no-multiline-string": false,
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 1,
        "maxEOF": 1
      }
    ],
    "no-shadowed-variable": false,
    "no-string-literal": false,
    "no-trailing-whitespace": true,
    "no-unused": true,
    "no-unused-expression": false,
    "no-var-keyword": true,
    "object-curly-newline": false,
    "object-curly-spacing": [
      true,
      "always"
    ],
    "object-literal-key-quotes": [
      true,
      "as-needed"
    ],
    "object-literal-shorthand": true,
    "object-literal-sort-keys": false,
    "one-variable-per-declaration": false,
    "prefer-const": true,
    "quotemark": [
      true,
      "single",
      "jsx-single",
      "avoid-escape"
    ],
    "react-this-binding-issue": false,
    "space-within-parens": [true, 0],
    "switch-default": true,
    "ter-arrow-body-style": true,
    "ter-func-call-spacing": [
      true,
      "never"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "map": "always",
          "functions": "never",
          "typeLiterals": "ignore",
          "imports": "ignore",
          "exports": "ignore"
        },
        "esSpecCompliant": true
      }
    ],
    "triple-equals": true,
    "variable-name": [
      true,
      "check-format",
      "allow-pascal-case"
    ],
    "whitespace": [
      true,
      "check-operator"
    ]
  },
  "rules": {
    "align": false,
    "array-type": false,
    "await-promise": false,
    "chai-vague-errors": false,
    "completed-docs": false,
    "export-name": false,
    "forin": false,
    "import-name": false,
    "interface-name": [true, "never-prefix"],
    "interface-over-type-literal": false,
    "jsx-boolean-value": false,
    "jsx-curly-spacing": [true, "never", { "allowMultiline": true }],
    "jsx-no-multiline-js": false,
    "jsx-no-lambda": false,
    "jsx-wrap-multiline": false,
    "match-default-export-name": false,
    "max-classes-per-file": [
      true,
      15
    ],
    "max-line-length": {
      "options": [
        120
      ]
    },
    "missing-jsdoc": false,
    "no-any": false,
    "no-boolean-literal-compare": false,
    "no-cookies": false,
    "no-default-export": false,
    "no-empty": false,
    "no-import-side-effect": false,
    "no-floating-promises": false,
    "no-for-in-array": false,
    "no-implicit-dependencies": false,
    "no-increment-decrement": false,
    "no-multiline-string": false,
    "no-relative-imports": false,
    "no-reserved-keywords": false,
    "no-shadowed-variable": false,
    "no-single-line-block-comment": false,
    "no-submodule-imports": false,
    "no-this-assignment": false,
    "no-unsafe-any": false,
    "no-unnecessary-qualifier": false,
    "no-unused-expression": false,
    "no-use-before-declare": false,
    "no-void-expression": false,
    "object-literal-sort-keys": false,
    "object-shorthand-properties-first": false,
    "one-variable-per-declaration": false,
    "only-arrow-functions": false,
    "prefer-method-signature": false,
    "promise-function-async": false,
    "quotemark": [true, "single", "jsx-single", "avoid-escape"],
    "react-this-binding-issue": false,
    "react-tsx-curly-spacing": [true, "never", { "allowMultiline": true }],
    "restrict-plus-operands": false,
    "strict-boolean-expressions": false,
    "ter-arrow-parens": false,
    "trailing-comma": false,
    "typedef": [
      true,
      "call-signature",
      "parameter",
      "member-variable-declaration"
    ],
    "variable-name": [
      true,
      "check-format",
      "allow-pascal-case"
    ],
    "whitespace": [
      true,
      "check-operator"
    ]
  },
  "rulesDirectory": []
}
